@model IEnumerable<TAN.DVDCentral.BL.Models.Genre>

 <ul>
        @foreach (var genre in Model)
    {
        <li>
            <a asp-controller="Movie"
               asp-action="Browse"
                asp-route-id= "@genre.Id">@genre.Description
            </a>
        </li>
        
    }
    <li>
        <a asp-controller="Movie"
           a asp-action="">All</a>
    </li>
</ul>
